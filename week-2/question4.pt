def twoSum(nums, target):
    length=len(nums)
    numsList= []
    for i in range(length):
        for j in range(1,length):
            sumNum = nums[i] + nums[j]
            if sumNum == target:
                numsList.append(i)
                numsList.append(j)
                return(numsList)

result=twoSum([2, 11, 7, 15], 9)
print(result) # show [0, 2] because nums[0]+nums[2] is 9
